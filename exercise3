package cat;

interface bobcat {

    String eyecolor = "brown";
    
}
package cat;

public class cat_run{
    public static void main(String[] args) throws Exception{

        pet mycat = new pet();

        mycat.sound();
        System.out.println(mycat.eyecolor);
        System.out.println(mycat.weight);



    }
}package cat;

public class pet implements pixiebob{

    public void sound(){
        System.out.println("My cute pixiebob meows");
    }
    
}package cat;

interface pixiebob extends bobcat,shorthair{

    void sound();
    
    
}
package cat;

interface shorthair {

    String weight = "10lbs";

    
}

